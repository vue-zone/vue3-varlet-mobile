{
  "name": "vue3-varlet-mobile",
  "type": "module",
  "version": "2.0.1",
  "packageManager": "pnpm@10.9.0",
  "description": "An mobile web apps template based on the Vue 3 ecosystem",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env MOCK_SERVER_PORT=8086 vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp --commit --push --tag",
    "typecheck": "vue-tsc --noEmit",
    "commitlint": "commitlint --edit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@unhead/vue": "^2.0.8",
    "@varlet/touch-emulator": "^3.10.5",
    "@varlet/ui": "^3.10.5",
    "@vueuse/core": "^13.1.0",
    "axios": "^1.9.0",
    "echarts": "^5.6.0",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.2",
    "pinia-plugin-persistedstate": "^4.2.0",
    "resize-detector": "^0.3.0",
    "vconsole": "^3.15.1",
    "vue": "^3.5.13",
    "vue-i18n": "^11.1.3",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.12.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@iconify-json/carbon": "^1.2.8",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.0",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-config": "66.1.0-beta.12",
    "@varlet/import-resolver": "^3.10.5",
    "@varlet/preset-unocss": "^3.10.5",
    "@varlet/unplugin-icon-builder": "^0.6.0",
    "@vitejs/plugin-legacy": "^6.1.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "autoprefixer": "^10.4.21",
    "bumpp": "^10.1.0",
    "consola": "^3.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.25.1",
    "eslint-plugin-format": "^1.0.1",
    "less": "^4.3.0",
    "lint-staged": "^15.5.1",
    "mockjs": "^1.1.0",
    "postcss-mobile-forever": "^5.0.0",
    "rollup": "^4.40.0",
    "simple-git-hooks": "^2.13.0",
    "terser": "^5.39.0",
    "typescript": "^5.8.3",
    "unocss": "66.1.0-beta.12",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "unplugin-vue-router": "^0.12.0",
    "vite": "^6.3.3",
    "vite-plugin-mock-dev-server": "^1.8.5",
    "vite-plugin-pwa": "^1.0.0",
    "vite-plugin-sitemap": "^0.7.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.7.5",
    "vue-tsc": "^2.2.10"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "glob": "7.2.3",
      "inflight": "1.0.6",
      "sourcemap-codec": "1.4.8",
      "@xmldom/xmldom": "0.7.13"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@varlet/preset-unocss>unocss": "66.1.0-beta.12",
        "@varlet/unplugin-icon-builder>rollup": "4.40.0"
      }
    },
    "onlyBuiltDependencies": [
      "core-js",
      "esbuild",
      "simple-git-hooks",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "resolutions": {
    "vite": "^6.3.3"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "pnpm commitlint $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
