{
  "name": "vue3-varlet-mobile",
  "type": "module",
  "version": "2.0.1",
  "packageManager": "pnpm@9.3.0",
  "description": "Vue + Vite H5 Starter Template",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env MOCK_SERVER_PORT=8086 vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:dev": "vue-tsc --noEmit && vite build --mode=development",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "release": "bumpp --commit --push --tag",
    "prepare": "husky"
  },
  "dependencies": {
    "@unhead/vue": "^1.11.10",
    "@varlet/touch-emulator": "^3.6.3",
    "@varlet/ui": "^3.6.3",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "echarts": "^5.5.1",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.4",
    "pinia-plugin-persistedstate": "^4.1.2",
    "resize-detector": "^0.3.0",
    "store": "^2.0.12",
    "vconsole": "^3.15.1",
    "vue": "^3.5.12",
    "vue-i18n": "^10.0.4",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "3.8.0",
    "@iconify-json/carbon": "^1.2.4",
    "@intlify/unplugin-vue-i18n": "^5.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.8.2",
    "@types/nprogress": "^0.2.3",
    "@types/store": "^2.0.5",
    "@unocss/eslint-plugin": "0.63.6",
    "@unocss/preset-rem-to-px": "0.63.6",
    "@varlet/import-resolver": "^3.6.3",
    "@varlet/preset-unocss": "^3.6.3",
    "@varlet/unplugin-icon-builder": "^0.5.0",
    "@vitejs/plugin-legacy": "^5.4.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "autoprefixer": "^10.4.20",
    "bumpp": "^9.8.0",
    "consola": "^3.2.3",
    "cross-env": "^7.0.3",
    "eslint": "^9.12.0",
    "eslint-plugin-format": "^0.1.2",
    "husky": "^9.1.6",
    "less": "^4.2.0",
    "mockjs": "^1.1.0",
    "postcss-mobile-forever": "^4.2.3",
    "rollup": "^4.24.2",
    "terser": "^5.36.0",
    "typescript": "^5.6.3",
    "unocss": "0.63.6",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.10",
    "vite-plugin-mock-dev-server": "^1.8.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-sitemap": "^0.7.1",
    "vite-plugin-vconsole": "^2.1.1",
    "vite-plugin-vue-devtools": "^7.5.4",
    "vitest": "^2.1.4",
    "vue-tsc": "^2.1.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss",
        "esbuild"
      ],
      "allowedVersions": {
        "rollup": "^4.x",
        "unocss": "^0.x"
      },
      "allowAny": [
        "eslint"
      ]
    }
  }
}
